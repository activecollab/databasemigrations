{
    "name": "activecollab/databasemigrations",
    "type": "library",
    "description": "Database schema migrations",
    "license": "MIT",
    "keywords": [
        "migrations",
        "mysql",
        "activecollab"
    ],
    "homepage": "https://labs.activecollab.com",
    "authors": [
        {
            "name": "Ilija Studen",
            "email": "ilija.studen@activecollab.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "activecollab/databaseconnection": "^5.0",
        "activecollab/filesystem": "^0.10",
        "doctrine/inflector": "^2.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "ext-mysqli": "*",
        "friendsofphp/php-cs-fixer": "^2.0",
        "monolog/monolog": "^1.0",
        "phpunit/phpunit": "^9.0",
        "symfony/console": "^4.0"
    },
    "suggest": {
        "symfony/console": "Quickly implement commands that work with migrations via CLI"
    },
    "autoload": {
        "psr-4": {
            "ActiveCollab\\DatabaseMigrations\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ActiveCollab\\DatabaseMigrations\\Test\\": "test/src"
        }
    }
}