{
    "name": "activecollab/databasemigrations",
    "type": "library",
    "description": "Database schema migrations",
    "license": "MIT",
    "prefer-stable": true,
    "keywords": [
        "migrations",
        "mysql",
        "activecollab"
    ],
    "homepage": "https://labs.activecollab.com",
    "authors": [
        {
            "name": "Ilija Studen",
            "email": "ilija.studen@activecollab.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "activecollab/databasestructure": "dev-master",
        "activecollab/filesystem": "~0.1",
        "doctrine/inflector": "~1.0",
        "psr/log": "~1.0.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.0",
        "monolog/monolog": "^1.0",
        "phpunit/phpunit": "^5.0",
        "symfony/console": "^2.7"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "ActiveCollab\\DatabaseMigrations\\": "src",
            "ActiveCollab\\DatabaseMigrations\\Test\\": "test/src"
        }
    }
}